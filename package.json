{
  "name": "sauce4zwift",
  "productName": "Sauce for Zwift™",
  "version": "0.0.2",
  "description": "Overlay for Zwift™",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "start-debug-brk": "electron --inspect-brk .",
    "start-debug": "electron --inspect ."
  },
  "author": "Sauce, LLC",
  "license": "MIT",
  "dependencies": {
    "@saucellc/zwift-packet-monitor": "https://github.com/SauceLLC/zwift-packet-monitor.git",
    "electron-notarize": "1.0.0",
    "sudo-prompt": "9.2.1"
  },
  "devDependencies": {
    "electron": "^13",
    "electron-builder": "22.11.7",
    "eslint": "^7.27.0"
  },
  "build": {
    "appId": "io.saucellc.sauce4zwift",
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": "mas",
      "icon": "build/images/app-icon.icns",
      "provisioningProfile": "../../Documents/Sauce.provisionprofile",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "afterSign": "build/scripts/notarize.js"
  }
}
